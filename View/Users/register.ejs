<section class="login_part  ">
    <div class="container">
        <div class="row align-items-center">
            <div class="col-lg-6 col-md-6" >
                <div class="login_part_form">
                    <div class="login_part_form_iner">
                        <h3>Create new account here ! <br>
                            Please Register now Mushrif@123 </h3>
                        <form class="row contact_form" action="/registerPost" method="post"  >
                            <div class="col-md-12 form-group p_star">
                                <input type="text" class="form-control" id="name" name="name" value="<%= typeof currentName !== "undefined" ? currentName : "" %>"
                                    placeholder="Username" required>
                            </div>
                            <div class="col-md-12 form-group p_star">
                                <input type="email" class="form-control" id="email" name="email" value="<%= typeof currentEmail !== "undefined" ? currentEmail : "" %>"
                                    placeholder="Email" required>
                            </div>
                            <div class="col-md-12 form-group p_star">
                                <input type="number" class="form-control" id="number" name="number" value="<%= typeof currentPhone !== "undefined" ? currentPhone : "" %>"
                                    placeholder="Phone Number" required>
                            </div>
                            <div class="col-md-12 form-group p_star">
                                <input type="password" class="form-control" id="password" name="password" value="<%= typeof currentPass !== "undefined" ? currentPass : "" %>"
                                    placeholder="Password" required>
                            </div>
                            <div class="col-md-12 form-group p_star">
                                <input type="password" class="form-control" id="retypePassword" name="retypePassword" value="<%= typeof currentPass !== "undefined" ? currentPass : "" %>"
                                    placeholder="Retype password" required>
                            </div>

                                <% if(typeof message !== "undefined") { %>
                                    <p style="color: rgb(255, 0, 0);"><%= message %></p>
                                  <% } %>

                            <div class="col-md-12 form-group">
                                <button  id="registerButton"  type="submit" value="submit" class="btn_3">
                                    Register
                                </button>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
            <div class="col-lg-6 col-md-6">
                <div class="login_part_text text-center" style="box-shadow: 0 0 8px rgba(0, 0, 0, 0.617);">
                    <div class="login_part_text_iner">
                        <h2>Already have an account ?</h2>
                        <p>There are advances being made in science and technology everyday, and a good example of this is the</p>
                        <a href="/login" class="btn_3">click here to log in </a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!--================register_part end =================-->

